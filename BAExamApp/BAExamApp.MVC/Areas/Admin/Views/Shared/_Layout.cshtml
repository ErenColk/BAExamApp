<script>


    window.addEventListener('beforeunload', function (event) {
        startLoader();
    });

    window.addEventListener("load", function (event) {
        stopLoader();
    });

    function handleAccordionMenuClick(menuName) {
        localStorage.setItem('selectedAccordionMenu', menuName);


        var clickedMenu = document.getElementById(menuName + 'AccordionMenu');


        if (clickedMenu) {

            var allMenus = document.querySelectorAll('.menu-item menu-accordion');
            allMenus.forEach(function (menu) {
                menu.classList.remove('active');
            });


            clickedMenu.classList.add('active');
        }
    }

    function handleMenuItemClick(menuItemName) {
        localStorage.setItem('selectedMenuItem', menuItemName);

        var clickedMenuItem = document.getElementById(menuItemName);


        if (clickedMenuItem) {

            var allMenuItems = document.querySelectorAll('.menu-item');
            allMenuItems.forEach(function (item) {
                item.classList.remove('active');
            });


            clickedMenuItem.classList.add('show');

            var accordionMenu = clickedMenuItem.closest('.menu-item menu-accordion');

            accordionMenu = document.getElementById(localStorage.getItem('selectedAccordionMenu'));

            var allAccordionMenus = document.querySelectorAll('.menu-item menu-accordion');
            allAccordionMenus.forEach(function (menu) {

                menu.classList.remove('active');
            });
            accordionMenu.classList.add('show');
            accordionMenu.classList.add('hover');
        }

    }


    document.addEventListener("DOMContentLoaded", function () {

        document.getElementById(localStorage.getItem('selectedMenuItem')).firstElementChild.classList.add('active');
        document.getElementById(localStorage.getItem('selectedAccordionMenu')).classList.add('show');
        document.getElementById(localStorage.getItem('selectedAccordionMenu')).classList.add('hover');
        // localStorage.setItem('selectedAccordionMenu', null);
        // localStorage.setItem('selectedMenuItem', null);

    });

</script> 